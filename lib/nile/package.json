{
  "version": "0.11.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "packageManager": "yarn@3.2.0",
  "files": [
    "dist",
    "src",
    "spec",
    "bin/*.mjs",
    "templates",
    "scripts/api-cleaner.sh",
    "tsconfig.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build:types",
    "prebuild": "yarn build:api:gen",
    "build:readme": "./scripts/lib-readme-shim.sh",
    "build:types": "tsc -d --declarationDir dist --emitDeclarationOnly",
    "build:api:merge": "yarn openapi-merge-cli",
    "build:api:gen": "yarn openapi-generator-cli generate -t templates -i spec/api.yaml -g typescript-fetch --package-name nile -o src/generated/openapi --additional-properties=ngVersion=6.1.7,npmName=theniledev,supportsES6=true,npmVersion=6.9.0,withInterfaces=true,withSeparateModelsAndApi=true,moduleName=Nile,typescriptThreePlus=true,projectName=@theniledev/js",
    "test": "tsdx test",
    "prepare": "yarn prebuild && tsdx build && yarn build:types",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "bin": {
    "nile": "./bin/nile.mjs"
  },
  "name": "@theniledev/js",
  "author": "jrea",
  "module": "dist/js.esm.js",
  "size-limit": [
    {
      "path": "dist/js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.32",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.16",
    "typedoc-plugin-markdown": "^3.12.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNileDev/nile-js.git",
    "directory": "lib/nile"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "es6-promise": "^4.2.8",
    "node-fetch": "^3.2.3",
    "sade": "^1.8.1"
  }
}
